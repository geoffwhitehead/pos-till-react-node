{
  "name": "pos-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "dev:ios": "react-native run-ios --simulator='iPad Pro (12.9-inch) (4th generation)'",
    "dev:ios:device": "react-native run-ios --device='iPad 12.9'",
    "dev:app": "concurrently \"yarn start\" \"yarn dev:ios\"",
    "pod": "cd ./ios && pod install && cd .. ",
    "start": "react-native start",
    "test": "jest --detectOpenHandles",
    "prettier-fix": "prettier --write **/*.js",
    "prettier-check": "prettier --list-different **/*.js",
    "lint-fix": "eslint --fix .",
    "lint-check": "eslint .",
    "rename": "react-native-rename",
    "postinstall": "npx jetify",
    "find:watermelon": "find ~/Library -type f -name 'watermelon.db'",
    "reset:cache": "watchman watch-del-all && react-native start --reset-cache",
    "set:local": "rm -rf ./build.env.js && echo 'export default \"local\";' >> build.env.js",
    "set:production": "rm -rf ./build.env.js && echo 'export default \"production\";' >> build.env.js"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.20.0",
    "@nozbe/with-observables": "^1.0.5",
    "@react-native-community/async-storage": "^1.8.0",
    "@react-native-community/datetimepicker": "^3.0.5",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-navigation/drawer": "^5.0.5",
    "@react-navigation/native": "^5.0.0",
    "@react-navigation/stack": "^5.0.0",
    "apisauce": "^1.1.1",
    "async-retry": "^1.3.1",
    "axios": "^0.21.1",
    "babel-preset-react-native": "^4.0.1",
    "dayjs": "^1.8.21",
    "formik": "^2.1.4",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^3.5.0",
    "native-base": "^2.13.14",
    "observable-hooks": "^2.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-native": "0.61.5",
    "react-native-color": "^0.0.10",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-modal": "^11.5.4",
    "react-native-modal-datetime-picker": "^9.1.0",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^0.7.2",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-star-prnt": "https://github.com/geoffwhitehead/react-native-star-prnt.git",
    "react-native-svg": "^12.1.0",
    "react-native-swipe-list-view": "^3.1.0",
    "react-native-switch-selector": "^2.0.6",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.2",
    "reduxsauce": "^1.0.1",
    "rxjs-compat": "^6.6.3",
    "tinycolor2": "^1.4.2",
    "uuid": "^8.3.1",
    "victory-native": "^35.3.1",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/lodash": "^4.14.155",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.12",
    "@types/react-navigation": "^3.4.0",
    "@types/yup": "^0.26.32",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@welldone-software/why-did-you-render": "^6.0.3",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.19.1",
    "react-native-rename": "^2.4.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native)/.+"
    ]
  }
}
